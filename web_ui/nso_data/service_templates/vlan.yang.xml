<?xml version="1.0" encoding="UTF-8"?>
<module name="vlan"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:vlan="http://com/example/vlan"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types">
  <namespace uri="http://com/example/vlan"/>
  <prefix value="vlan"/>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="tailf-ncs">
    <prefix value="ncs"/>
  </import>
  <augment target-node="/ncs:services">
    <list name="vlan">
      <key value="name"/>
      <uses name="ncs:service-data"/>
      <ncs:servicepoint/>
      <leaf name="name">
        <type name="string"/>
      </leaf>
      <leaf name="vlan-id">
        <type name="uint32">
          <range value="1..4096"/>
        </type>
      </leaf>
      <list name="device-if">
        <key value="device-name"/>
        <leaf name="device-name">
          <type name="leafref">
            <path value="/ncs:devices/ncs:device/ncs:name"/>
          </type>
        </leaf>
        <leaf name="interface-type">
          <type name="enumeration">
            <enum name="FastEthernet"/>
            <enum name="GigabitEthernet"/>
            <enum name="TenGigabitEthernet"/>
          </type>
        </leaf>
        <leaf name="interface">
          <type name="string"/>
        </leaf>
      </list>
    </list>
  </augment>
</module>
