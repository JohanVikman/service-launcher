<?xml version="1.0" encoding="UTF-8"?>
<module name="basic-config"
        xmlns="urn:ietf:params:xml:ns:yang:yin:1"
        xmlns:basic-config="http://com/example/basicconfig"
        xmlns:inet="urn:ietf:params:xml:ns:yang:ietf-inet-types">
  <namespace uri="http://com/example/basicconfig"/>
  <prefix value="basic-config"/>
  <import module="ietf-inet-types">
    <prefix value="inet"/>
  </import>
  <import module="tailf-ncs">
    <prefix value="ncs"/>
  </import>
  <augment target-node="/ncs:services">
    <list name="basic-config">
      <key value="name"/>
      <uses name="ncs:service-data"/>
      <ncs:servicepoint/>
      <leaf name="name">
        <type name="string"/>
      </leaf>
      <leaf-list name="device">
        <type name="leafref">
          <path value="/ncs:devices/ncs:device/ncs:name"/>
        </type>
      </leaf-list>
      <leaf name="hostname">
        <type name="string"/>
      </leaf>
      <leaf name="domain-name-server">
        <type name="inet:ipv4-address"/>
      </leaf>
    </list>
  </augment>
</module>
